{
  "DetailedErrors": true,
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "Hathor"
    }
  },
  "ConnectionStrings": {
    "Default": "Server=localhost;Database=Hathor;Integrated Security=SSPI;"
  }
}
